{
  "name": "@dill0wn/pixi-flex-layout",
  "author": "@dill0wn",
  "version": "1.1.0",
  "description": "Flex layouts for pixi.js powered by Yoga",
  "keywords": [
    "pixi.js",
    "pixi",
    "layout",
    "ui",
    "flex",
    "yoga"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/dill0wn/pixi-flex-layout.git"
  },
  "scripts": {
    "start": "parcel serve test/test.html",
    "bundle:examples": "parcel build test/test.html",
    "build": "tsc",
    "watch": "parcel watch test/test.html",
    "generate-doc": "rimraf -f doc/* && typedoc --out doc/ src/index.ts --ignoreCompilerErrors --mode file  --excludeNotExported --exclude 'test/*' --exclude 'node_modules/'"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/offscreencanvas": "^2019.6.4",
    "@types/yoga-layout": "^1.9.1",
    "javascript-editor": "https://github.com/dill0wn/javascript-editor",
    "parcel-bundler": "^1.12.4",
    "pixi.js": "^6.2.2",
    "typedoc": "^0.17.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "yoga-layout-prebuilt": "^1.10.0"
  }
}
