{
  "name": "pixi-flex-layout",
  "author": "Matuesz Szymik@Fireveined",
  "version": "1.0.13",
  "description": "Flex layouts for pixi.js powered by Yoga",
  "keywords": [
    "pixi.js",
    "pixi",
    "layout",
    "ui",
    "flex",
    "yoga"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fireveined/pixi-flex-layout.git"
  },
  "scripts": {
    "start": "parcel serve test/test.html",
    "bundle:examples": "parcel build test/test.html",
    "build": "tsc",
    "watch": "parcel watch test/test.html",
    "generate-doc": "rimraf -f doc/* && typedoc --out doc/ src/index.ts --ignoreCompilerErrors --mode file  --excludeNotExported --exclude 'test/*' --exclude 'node_modules/'"
  },
  "license": "MIT",
  "peerDependencies": {
    "pixi.js": "^6.2.2"
  },
  "devDependencies": {
    "@types/yoga-layout": "^1.9.1",
    "javascript-editor": "^1.0.3",
    "parcel-bundler": "^1.12.4",
    "typedoc": "^0.17.3",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "yoga-layout-prebuilt-low-memory": "*"
  }
}
